<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/succursale_nav_graph"
    app:startDestination="@id/connexionFragment">

    <fragment
        android:id="@+id/connexionFragment"
        android:name="ca.qc.cgodin.miniprojet3mobilecegep.ConnexionFragment"
        android:label="fragment_connexion"
        tools:layout="@layout/fragment_connexion" >
        <action
            android:id="@+id/action_connexionFragment_to_enregistrementFragment"
            app:destination="@id/enregistrementFragment" />
        <action
            android:id="@+id/action_connexionFragment_to_listSuccursaleFragment"
            app:destination="@id/listSuccursaleFragment" />
    </fragment>
    <fragment
        android:id="@+id/listSuccursaleFragment"
        android:name="ca.qc.cgodin.miniprojet3mobilecegep.ListSuccursaleFragment"
        android:label="fragment_list_succursale"
        tools:layout="@layout/fragment_list_succursale" >
        <argument
            android:name="aut"
            app:argType="string" />
        <action
            android:id="@+id/action_listSuccursaleFragment_to_connexionFragment"
            app:destination="@id/connexionFragment" />
    </fragment>
    <fragment
        android:id="@+id/enregistrementFragment"
        android:name="ca.qc.cgodin.miniprojet3mobilecegep.EnregistrementFragment"
        android:label="fragment_enregistrement"
        tools:layout="@layout/fragment_enregistrement" >
        <action
            android:id="@+id/action_enregistrementFragment_to_connexionFragment"
            app:destination="@id/connexionFragment" />
    </fragment>
    <fragment
        android:id="@+id/operationSuccursaleFragment"
        android:name="ca.qc.cgodin.miniprojet3mobilecegep.OperationSuccursaleFragment"
        android:label="fragment_operation_succursale"
        tools:layout="@layout/fragment_operation_succursale" />
</navigation>